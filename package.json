{
	"name": "vale-rocks",
	"type": "module",
	"dependencies": {
		"@weborigami/origami": "0.2.1"
	},
	"scripts": {
		"dev": "ori serve watch src, =debug src/site.ori",
		"build": "ori copy src/site.ori, clear files:build && bun run sitemap",
