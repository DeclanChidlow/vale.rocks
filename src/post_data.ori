posts
→ (_) => Tree.map(_, (post, fileName, _body) => {
	...post
	type: "post"
	file_name: Origami.basename(fileName)
	words: functions/word-count.js(post._body)
})
→ (_) => Tree.sort(_, (post) => post/pub_time)
→ (_) => Tree.map(_, {
	extension: ".md->.html"
	value: (post) => functions/parse-markdown.js(post)
})
→ Tree.reverse
